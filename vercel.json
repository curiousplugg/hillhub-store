{
  "version": 2,
  "name": "hillhub",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_ENV": "production"
  },
  "functions": {
    "src/app/api/webhooks/stripe/route.ts": {
      "maxDuration": 30
    }
  }
} 